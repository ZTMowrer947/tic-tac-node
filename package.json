{
  "name": "tic-tac-node",
  "version": "1.0.0",
  "description": "A simple tic-tac-toe program",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "esbuild src/index.ts --platform=node --packages=external --outdir=dist --bundle --minify --format=esm",
    "start": "node dist",
    "typecheck": "tsc -noEmit",
    "typecheck:watch": "tsc -noEmit -w",
    "test": "jest",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZTMowrer947/tic-tac-node.git"
  },
  "author": "Zackery Mowrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZTMowrer947/tic-tac-node/issues"
  },
  "homepage": "https://github.com/ZTMowrer947/tic-tac-node#readme",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^14.0.12",
    "@types/readline-sync": "^1.4.3",
    "esbuild": "^0.21.5",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  },
  "dependencies": {
    "readline-sync": "^1.4.10"
  },
  "packageManager": "yarn@4.3.0"
}
